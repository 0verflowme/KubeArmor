apiVersion: security.accuknox.com/v1
kind: KubeArmorPolicy
metadata:
  name: ksp-group-2-proc-path-source-block
  namespace: multiubuntu
spec:
  selector:
    matchLabels:
      group: group-2
  process:
    matchPaths:
    - path: /bin/sleep # try sleep 1
      fromSource:
      - path: /bin/sh
  action:
    Block
